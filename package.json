{
  "name": "@vue-email/nuxt",
  "type": "module",
  "version": "0.8.19",
  "packageManager": "pnpm@9.1.1",
  "description": "Nuxt package for vue-email",
  "author": "David Arenas <davejs136@gmail.com> (https://github.com/Dave136/)",
  "license": "MIT",
  "homepage": "https://vuemail.net/",
  "maintainers": [
    {
      "name": "Dave136",
      "email": "davejs136@gmail.com",
      "url": "https://github.com/Dave136"
    },
    {
      "name": "Flowko",
      "email": "younesbarraddev@gmail.com",
      "url": "https://github.com/Flowko"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "build": "pnpm dev:prepare && pnpm prepack",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:preview": "nuxi preview playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "release": "bumpp"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "defu": "^6.1.4",
    "destr": "^2.0.3",
    "json5": "^2.2.3",
    "sirv": "^2.0.4",
    "vue-component-meta": "^2.0.19",
    "vue-email-edge": "0.8.11-28628304.31f4052"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.7.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.14.1",
    "bumpp": "^9.4.1",
    "destr": "^2.0.3",
    "eslint": "^9.4.0",
    "jiti": "^1.21.0",
    "nuxt": "^3.11.2",
    "vitest": "^1.6.0"
  }
}
